{
  "name": "blocknote-vue",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.14.0",
  "description": "Vue interface for Blocknote",
  "author": "Huali <zcf0508@live.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/zcf0508/blocknote-vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zcf0508/blocknote-vue.git"
  },
  "bugs": "https://github.com/zcf0508/blocknote-vue/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "play": "npm -C playground run dev",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@blocknote/core": "^0.12.0",
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@blocknote/core": "^0.12.0",
    "@iconify/json": "^2.2.177",
    "@vueuse/core": "^10.7.2",
    "radix-vue": "^1.3.2",
    "unocss": "^0.58.4",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite-plugin-dts": "^3.7.2",
    "vue-demi": "^0.14.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.0",
    "@floating-ui/vue": "^1.0.4",
    "@types/node": "^20.10.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-vue": "^9.21.1",
    "eslint-plugin-vuejs-accessibility": "^2.2.1",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.14.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.3",
    "vue": "^3.4.15",
    "vue-eslint-parser": "^9.4.2"
  }
}
